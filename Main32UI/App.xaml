<Application x:Class="Main32UI.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:Main32UI"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <!--<ResourceDictionary Source="/Styles/HIDDemoStyle.xaml"/>-->
                <!-- Resource Order
                https://nathan.alner.net/2010/05/07/wpf-style-and-template-resources-ndash-order-matters/-->
                <ResourceDictionary Source="pack://application:,,,/WPFCommonLib;component/Styles/CommonConstants.xaml"/>
                <ResourceDictionary Source="pack://application:,,,/WPFCommonLib;component/Styles/CommonStyle.xaml"/>
                <ResourceDictionary Source="pack://application:,,,/WPFCommonLib;component/Styles/CommonControlTemplate.xaml"/>
                <ResourceDictionary Source="pack://application:,,,/WPFCommonLib;component/Styles/CommonDataTemplate.xaml"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
